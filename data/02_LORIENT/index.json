{
  "id": "cense",
  "name": "ANR CENSE",
  "description": " Les cartes de paysages sonores présentent les résultats d'une enquête réalisée auprès des habitants de la zone d'étude (du centre ville de Lorient).",
  "image": "logo.jpeg",
  "sources": {
    "bdTopo": {
      "attribution": "IGN",
      "type": "vector",
      "tiles": [
        "https://wxs.ign.fr/topographie/geoportail/tms/1.0.0/BDTOPO/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 4,
      "maxzoom": 19
    },
    "osm": {
      "attribution": "OSM",
      "type": "raster",
      "tiles": [
        "https://tile.openstreetmap.org/{z}/{x}/{y}.png"
      ]
    },
    "Geo2": {
      "attribution": "Pierre",
      "type": "geojson",
      "data": "./layers/resultGrid2.geojson"
    }
  },
  "maps": [
    {
      "id": "Grid2_bird",
      "name": "Bird from Grid2",
      "layers": [
        {
          "id": "basemap",
          "type": "raster",
          "source": "osm"
        },
        {
          "id": "acoustic-birds",
          "source": "Geo2",
          "metadata": {
            "lasso:geojsonProperty": "meanBrd"
          },
          "type": "fill",
          "paint": {
            "fill-color": [
              "interpolate",
              [
                "linear"
              ],
              [
                "get",
                "meanBrd"
              ],
              0,
              [
                "to-color",
                "#edf8e9"
              ],
              7,
              [
                "to-color",
                "#006d2c"
              ]
            ]
          }
        },
        {
          "id": "bati",
          "type": "fill-extrusion",
          "source": "bdTopo",
          "source-layer": "batiment",
          "paint": {
            "fill-extrusion-height": {
              "property": "hauteur",
              "stops": [
                [
                  0,
                  2
                ],
                [
                  2,
                  5
                ],
                [
                  5,
                  10
                ],
                [
                  10,
                  20
                ],
                [
                  20,
                  40
                ],
                [
                  40,
                  60
                ],
                [
                  60,
                  80
                ],
                [
                  80,
                  100
                ],
                [
                  100,
                  200
                ]
              ]
            },
            "fill-extrusion-base": 0,
            "fill-extrusion-color": "#EEEEEE"
          }
        }
      ]
    }
  ]
}