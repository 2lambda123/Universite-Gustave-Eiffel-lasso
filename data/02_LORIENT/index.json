{
  "id": "cense",
  "name": "ANR CENSE",
  "description": " Les cartes de paysages sonores présentent les résultats d'une enquête réalisée auprès des habitants de la zone d'étude (du centre ville de Lorient).",
  "image": "logo.jpeg",
  "sources": {
    "Geo2": {
      "attribution": "Pierre",
      "type": "geojson",
      "data": "./layers/resultGrid2.geojson",
      "variables": {
        "acoustic-birds": "meanBrd",
        "acoustic-trafic": "meanTra"
      }
    }
  },
  "maps": [
    {
      "id": "Grid2_bird",
      "name": "Bird from Grid2",
      "basemapStyle": "./OSM_BDTOPO.style.json",
      "layers": [
        {
          "id": "acoustic-birds",
          "source": "Geo2",
          "beforeId": "bati",
          "metadata": {
            "interactive": true
          },
          "type": "fill",
          "paint": {
            "fill-color": [
              "interpolate",
              [
                "linear"
              ],
              [
                "get",
                "acoustic-birds"
              ],
              0,
              [
                "to-color",
                "#edf8e9"
              ],
              7,
              [
                "to-color",
                "#006d2c"
              ]
            ]
          }
        }
      ]
    },
    {
      "id": "Grid2_traf",
      "name": "Trafic",
      "basemapStyle": "./OSM_BDTOPO.style.json",
      "layers": [
        {
          "id": "acoustic-trafic",
          "source": "Geo2",
          "beforeId": "bati",
          "metadata": {
            "interactive": true
          },
          "type": "fill",
          "paint": {
            "fill-color": [
              "interpolate",
              [
                "linear"
              ],
              [
                "get",
                "acoustic-trafic"
              ],
              0,
              [
                "to-color",
                "#FFDDDD"
              ],
              7,
              [
                "to-color",
                "#F22"
              ]
            ]
          }
        }
      ]
    }
  ]
}