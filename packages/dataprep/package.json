{
  "name": "@lasso/dataprep",
  "version": "1.0.0",
  "private": true,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@types/papaparse": "^5.3.2"
  },
  "dependencies": {
    "papaparse": "^5.3.2"
  },
  "scripts": {
    "clean": "rimraf lib/",
    "lint": "esw --ext .ts ./src",
    "compile": "tsc -b",
    "test": "mocha --recursive --fail-zero 'false' --require ts-node/register 'test/**/*.ts' ",
    "build": "npm run clean && npm run lint && npm run compile",
    "start": "concurrently -n lint,compile,exec \"npm run lint -- --watch\" \"npm run compile -- --watch\" \"npm run exec -- --watch\"",
    "exec": "ts-node ./src/index.ts"
  }
}
